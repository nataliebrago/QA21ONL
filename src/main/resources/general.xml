<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Main Suite">
    <test name="Smoke">
        <classes>
            <class name="CalculatorTestHW">
                <methods>
                    <include name="test1_int"/>
                    <include name="test1_double"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Regression">
        <classes>
            <class name="CalculatorTestHW">
                <methods>
                    <exclude name="test1_int"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>